    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "body-parser": "*",
    "mongodb": "^3.0.1",
    "mongoose": "*"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
